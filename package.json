{
  "name": "@yqg/cli",
  "version": "0.0.17",
  "description": "yqg web cli tool",
  "author": "yqg web team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.yangqianguan.com:web/common/yqg-cli.git"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.39",
    "chalk": "2.3.1",
    "commander": "2.14.1",
    "git-user-info": "1.0.0",
    "gulp": "3.9.1",
    "gulp-rename": "1.2.2",
    "gulp-template": "5.0.0",
    "moment": "2.20.1",
    "prompt": "1.0.0",
    "strman": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.38",
    "@babel/preset-env": "7.0.0-beta.38",
    "@babel/preset-stage-3": "7.0.0-beta.38",
    "@babel/preset-typescript": "7.0.0-beta.38",
    "@types/node": "9.4.6",
    "@yqg/arcanist": "0.0.2",
    "@yqg/eslint-config": "0.0.4",
    "babel-eslint": "8.2.2",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "husky": "0.15.0-rc.8",
    "lint-staged": "7.0.0",
    "rollup": "0.56.3",
    "rollup-plugin-babel": "4.0.0-beta.0",
    "rollup-plugin-cleanup": "2.0.0",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-typescript": "0.8.1",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.7.0",
    "typescript": "2.7.2"
  },
  "bin": {
    "yqg": "bin/yqg",
    "yqg-angular": "bin/yqg-angular",
    "yqg-vue": "bin/yqg-vue",
    "yqg-shell": "bin/yqg-shell",
    "yqg-resource": "bin/yqg-resource"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w",
    "eslint:all": "eslint rollup.config.js 'src/**/*.js'",
    "tslint:all": "tslint -c tslint.json -t verbose 'src/**/*.ts'",
    "arc:eslint": "eslint",
    "arc:tslint": "tslint -c tslint.json -t verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "tslint -c tslint.json -t verbose",
      "git add"
    ]
  }
}
