    "yqg-resource": "bin/yqg-resource",
    "yqg-clean": "bin/yqg-clean",
    "yqg-copy": "bin/yqg-copy",
    "yqg-bundle": "bin/yqg-bundle",
    "yqg-build": "bin/yqg-build",
    "yqg-start": "bin/yqg-start"
  },
  "scripts": {
    "arc:eslint": "eslint",
    "arc:tslint": "tslint -c tslint.json -t verbose",
    "eslint:all": "eslint rollup.config.js 'src/**/*.js'",
    "tslint:all": "tslint -c tslint.json -t verbose 'src/**/*.ts'",
    "build": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "tslint -c tslint.json -t verbose",
      "git add"
    ]
  }
