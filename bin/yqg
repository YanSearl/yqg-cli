#!/usr/bin/env node
'use strict';

function _interopDefault (ex) { return (ex && (typeof ex === 'object') && 'default' in ex) ? ex['default'] : ex; }

require('@babel/polyfill');
var commander = _interopDefault(require('commander'));

var version = "0.0.17";

var DEBUG = !!process.env.DEBUG;

/**
 * @author panezhang
 * @date 08/02/2018-13:24
 * @file setup
 */
process.on('uncaughtException', function (err) {
  console.log(); // 换行

  if (DEBUG) {
    console.log('uncaughtException', err);
  } else {
    console.log('异常退出');
  }
});
process.on('unhandledRejection', function (err, promise) {
  console.log(); // 换行

  if (DEBUG) {
    console.log('unhandledRejection', err, promise);
  } else {
    console.log('已退出');
  }
});

/**
 * @author panezhang
 * @date 28/12/2017-19:19
 * @file yqg
 */
commander.version(version).command('angular', 'angular scaffold').command('vue', 'vue scaffold').command('shell', 'run yqg scripts').parse(process.argv);
